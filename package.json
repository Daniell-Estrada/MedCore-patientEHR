{
  "name": "medcore-patientehr",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "build": "babel src -d dist --copy-files",
    "test": "jest --runInBand --watchAll --detectOpenHandles",
    "prisma:gen:postgresql": "npx prisma generate --schema=./prisma/postgresql/schema.prisma",
    "prisma:gen:mongodb": "npx prisma generate --schema=./prisma/mongodb/schema.prisma",
    "prisma:migrate:postgresql": "npx prisma migrate dev --schema=./prisma/postgresql/schema.prisma",
    "prisma:migrate:mongodb": "npx prisma db push --schema=./prisma/mongodb/schema.prisma",
    "docker:build": "docker build -t ms-patient-ehr:latest . ",
    "docker:run": "docker run -p 3003:3003 ms-patient-ehr:latest"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.6",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/preset-env": "^7.28.3",
    "@jest/globals": "^30.2.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prisma": "^6.16.2"
  }
}
